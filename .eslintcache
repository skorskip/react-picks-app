ick={() => viewPicks()}>\n                <div class=\"card-header-text\">\n                    {firstname} {lastname}\n                </div>            \n            </Button>\n        </div>\n    )\n\n    const userPicksContent = userPicks.map((pick) => {\n        return(\n            <div className=\"teams-container\">\n                <div className=\"team-picks\">\n                    <Team\n                        team={pick.awayTeam}\n                        locked={false}\n                        size=\"medium\"\n                        highlight={pick.awayTeam.team_id === pick.pick.team_id}\n                    />\n                    <div className=\"tiertary-colo game-icon\">\n                        @\n                    </div>\n                    <Team\n                        team={pick.homeTeam}\n                        locked={false}\n                        size=\"medium\"\n                        highlight={pick.homeTeam.team_id === pick.pick.team_id}\n                    />\n                </div>\n            </div>\n        )\n    })\n\n    const modalContent = (\n        <div className=\"modal-content\">\n            { userPicksContent }\n        </div>\n    )\n\n    const userModal = (showModal) && (\n        <div className=\"modal-container\" id=\"modal-container\">\n            <div className=\"user-modal base-background secondary-color\" id=\"user-modal\">\n                { modalHeader }\n                { modalInitIcon }\n                { modalTitle }\n                { modalContent }\n            </div>\n        </div>\n    );\n\n    useEffect(() => {\n        if(leagueState === 'complete') {\n            dispatch(fetchPicksForUser({userId: userId, season: league.currentSeason, seasonType: league.currentSeasonType, week: league.currentWeek}));\n        }\n    }, [userId, leagueState, league]);\n\n\n    return (\n        <>\n            <Subscriber topic={SHOW_MODAL}>\n                {data => (<>{showModalSub(data)}</>)}\n            </Subscriber>\n            { userModal }\n        </>\n    );\n}",{"ruleId":"193","replacedBy":"194"},{"ruleId":"195","replacedBy":"196"},{"ruleId":"197","severity":1,"message":"198","line":8,"column":10,"nodeType":"199","messageId":"200","endLine":8,"endColumn":14},{"ruleId":"197","severity":1,"message":"201","line":4,"column":10,"nodeType":"199","messageId":"200","endLine":4,"endColumn":22},{"ruleId":"202","severity":1,"message":"203","line":109,"column":8,"nodeType":"204","endLine":109,"endColumn":49,"suggestions":"205"},{"ruleId":"202","severity":1,"message":"206","line":109,"column":8,"nodeType":"204","endLine":109,"endColumn":37,"suggestions":"207"},"no-native-reassign",["208"],"no-negated-in-lhs",["209"],"no-unused-vars","'Icon' is defined but never used.","Identifier","unusedVar","'UserStanding' is defined but never used.","react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'league.currentSeason' and 'league.currentSeasonType'. Either include them or remove the dependency array.","ArrayExpression",["210"],"React Hook useEffect has a missing dependency: 'dispatch'. Either include it or remove the dependency array.",["211"],"no-global-assign","no-unsafe-negation",{"desc":"212","fix":"213"},{"desc":"214","fix":"215"},"Update the dependencies array to be: [dispatch, standingsStatus, leagueStatus, league.currentSeason, league.currentSeasonType]",{"range":"216","text":"217"},"Update the dependencies array to be: [userId, leagueState, league, dispatch]",{"range":"218","text":"219"},[3930,3971],"[dispatch, standingsStatus, leagueStatus, league.currentSeason, league.currentSeasonType]",[3589,3618],"[userId, leagueState, league, dispatch]"]